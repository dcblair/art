<% for (let i = 0; i < paintings.length; i++) { %>
  <% let randomNum = Math.floor(Math.random() * 60) %> 
  <div>
    <img src="<%= paintings[randomNum].image %>" alt="">
    <p><%= paintings[randomNum].title %> </p>
  </div>
<% } %>
